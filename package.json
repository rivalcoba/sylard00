{
  "name": "00-ehboilerplate",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "18.20.4",
    "npm": "10.7.0"
  },
  "scripts": {
    "build": "npm run clean && npm run b:s && npm run b:c",
    "b:c": "webpack --config ./webpack.config.babel.js --mode production",
    "b:r:p": "NODE_ENV=production npm run build && node ./dist-server/bin/www",
    "b:s": "babel ./server --out-dir dist-server --copy-files",
    "clean": "rimraf dist-server",
    "dev_old": "NODE_ENV=development babel-node ./server/bin/www",
    "p": "node ./dist-server/bin/www",
    "start": "NODE_ENV=production node ./dist-server/bin/www",
    "wp:s": "NODE_ENV=production webpack serve --config ./webpack.dev.config.js --mode develpment",
    "w:d": "NODE_ENV=development nodemon --ignore './client' --ignore './server/public/eaf' --exec babel-node ./server/bin/www",
    "dev": "NODE_ENV=development babel-watch --watch ./server/views ./server/bin/www",
    "ww:d": "SET NODE_ENV=development && nodemon --ignore './client' --exec babel-node ./server/bin/www",
    "www:d": "SET NODE_ENV=development && babel-node ./server/bin/www"
  },

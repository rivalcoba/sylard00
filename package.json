{
  "name": "00-ehboilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean transpile",
    "build:client": "webpack --mode=development",
    "clean": "rimraf dist-server",
    "dev": "NODE_ENV=development babel-node ./server/bin/www",
    "watch:dev": "NODE_ENV=development nodemon --exec babel-node ./server/bin/www",
    "prod": "NODE_ENV=production npm run build && node ./dist-server/bin/www",
    "start": "npm run prod",
    "transpile": "babel ./server --out-dir dist-server --copy-files"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^4.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}

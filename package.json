{
  "name": "00-ehboilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist-server/bin/www",
    "build": "npm run clean && npm run b:c && npm run b:s",
    "b:c": "webpack --mode=development",
    "b:s": "babel ./server --out-dir dist-server --copy-files",
    "clean": "rimraf dist-server",
    "dev": "NODE_ENV=development babel-node ./server/bin/www",
    "w:d": "NODE_ENV=development nodemon --ignore './client' --exec babel-node ./server/bin/www",
    "ww:d": "SET NODE_ENV=development && nodemon --ignore './client' --exec babel-node ./server/bin/www",
    "b:r:p": "NODE_ENV=production npm run build && node ./dist-server/bin/www",
    "p": "node ./dist-server/bin/www"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@fullstackjs/mail": "^1.0.7",
    "axios": "^0.19.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-handlebars": "^5.1.0",
    "express-session": "^1.17.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "i18n-express": "^1.1.3",
    "method-override": "^3.0.0",
    "mongoose": "^5.9.25",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.5",
    "rimraf": "^3.0.2",
    "sweetalert": "^2.1.2",
    "sweetalert2": "^9.17.1",
    "vue": "^2.6.12",
    "vue-axios": "^2.1.5",
    "wavesurfer.js": "^4.1.0",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.1",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "eslint": "^7.6.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "style-loader": "^1.2.1",
    "typescript": "^4.0.3",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
